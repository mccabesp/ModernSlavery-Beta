{
  "ApplicationInsights": {
    "InstrumentationKey": null
  },
  "BasicAuthentication": {
    "Enabled": false
  },
  "DUMP_SETTINGS": true,
  "Email": {
    "Providers": {
      "GovNotify": {
        "AllowTestKeyOnly": true
      }
    }
  },
  "GoogleAnalyticsAccountId": null,
  "Home": ".",
  "IdentityClient": {
    "AllowInvalidServerCertificates": true
  },
  "Logging": {
    "Console": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
  "OffsetCurrentDateTimeForSite": "0",
  "Pangolin": {
    "AppBaseUrl": "https://localhost:5001/",
    //"Download.Url": "c:\\download",
    "Browser": "Chrome",
    "Headless": "false"
  },
  "PinInPostTestMode": true,
  "ResponseCaching": {
    "Enabled": false //Turn off response caching for testing
  },
  "SearchService": {
    "ServiceName": "ss-test-uk-south"
  },
  "SecurityHeaders": {
    "Strict-Transport-Security": null //This is required to prevent blocking of http on localhost
  },
  "ShowEmailVerifyLink": true,
  "SkipSpamProtection": "true",
  "Storage": {
    "AzureConnectionString": "UseDevelopmentStorage=true"
    //"LocalStorageRoot": ".\\Temp\\Test\\"
  },
  "TrustedIPDomains": "LocalHost",
  "urls": "https://$(WEBSITE_HOSTNAME)", //localhost:0 forces kestrel to use a dynamic port especially
  "UseDeveloperExceptions": "true",
  "WEBSITE_HOSTNAME": "localhost:5001"
}
