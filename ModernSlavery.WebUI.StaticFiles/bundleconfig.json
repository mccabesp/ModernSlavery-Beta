[
  // bundle css
  {
    "outputFileName": "wwwroot/assets/stylesheets/application.min.css",
    "inputFiles": [ "wwwroot/assets/stylesheets/**/!(*.min).css" ],
    "minify": {
      "enabled": true
    }
  },
  // bundle js
  {
    "outputFileName": "wwwroot/assets/javascripts/application.min.js",
    "inputFiles": [ "wwwroot/assets/javascripts/**/!(*.min).js" ],
    "minify": {
      "enabled": true,
      "renameLocals": true
    }
  }
]