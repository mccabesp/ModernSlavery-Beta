{
  "Features": {
    "DevOps": { "Action": "Enable" },
    "MetaRobots": { "Action": "Enable" }
  },
  "Home": "$(BaseDirectory)",
  "IdentityClient": {
    "AllowInvalidServerCertificates": true
  },
  "ResponseCaching": {
    "Enabled": true
  },
  "SearchService": {
    "ServiceName": "ss-dev-uk-south"
  },
  "SecurityHeaders": {
    "Strict-Transport-Security": "max-age=60; includeSubDomains"
  },
  "TestOptions": {
    "DUMP_SETTINGS": true,
    "ShowPinInPost": true,
    "ShowEmailVerifyLink": true
  },
  "UseDeveloperExceptions": "true"
}